#===================================================
# Get rate.
record(ai, "$(P){$(DEV)-Ax:A}RATE:RBV") {
  field(DTYP, "stream")
  field(INP, "@pump33dds.proto getStatusRateAxA($(P), $(DEV), $(CMD)) $(PORT)")
  field(PREC, "0")
  field(SDIS, "$(P)$(R)DISABLE")
  field(SCAN,"1 second")
}


