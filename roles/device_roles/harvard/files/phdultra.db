record(ao, "$(P)$(R):POLLON") {
  field(DESC, "Set Polling")
  field(DTYP, "stream")
  field(OUT, "@phdultra.proto setPollOn($(P)$(R)) $(PORT)")
  field(SDIS, "$(P)$(R):DISABLE")
  field(PINI, "YES")
}

record(bo, "$(P)$(R):DISABLE") {
  field(DESC, "Disable comms")
  field(PINI, "YES")
  field(VAL, "$(DISABLE=1)")
  field(OMSL, "supervisory")
  field(ZNAM, "Enabled")
  field(ONAM, "Disabled")
  field(FLNK, "$(P)$(R):POLLON")
}

record(seq, "$(P)$(R):UPDATE") {
  field(DESC, "Update Temp Params")
  field(DOL1, "1")
  field(DOL2, "1")
  field(DOL3, "1")
  field(DOL4, "1")
  field(DOL5, "1")
  field(DOL6, "1")
  field(DOL7, "1")
  field(DOL8, "1")
  field(LNK1, "$(P)$(R):FORCE:RBV.PROC")
  field(LNK2, "$(P)$(R):TTIME:RBV.PROC")
  field(LNK3, "$(P)$(R):DIAMETER:RBV.PROC")
  field(LNK4, "$(P)$(R):IRATE:RBV.PROC")
  field(LNK5, "$(P)$(R):WRATE:RBV.PROC")
  field(LNK6, "$(P)$(R):IVOLUME:RBV.PROC")
  field(LNK7, "$(P)$(R):WVOLUME:RBV.PROC")
  field(LNK8, "$(P)$(R):TVOLUME:RBV.PROC")
  field(SCAN, "1 second")
  field(PINI, "YES")
}

