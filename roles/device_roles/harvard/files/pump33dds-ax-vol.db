record(ao, "$(P){$(DEV)-Ax:$(AX)}$(CMD)") {
  field(DESC, "Axis $(AX) $(CMD)")
  field(DTYP, "stream")
  field(FLNK, "$(P){$(DEV)-Ax:$(AX)}$(CMD):RBV")
  field(OUT, "@pump33dds.proto setVolAx($(P), $(DEV), $(AX), $(CMD)) $(PORT)")
  field(PREC, "0")
  field(DRVH, "100")
  field(DRVL, "0")
  field(SDIS, "$(P)$(R)DISABLE")
  info(autosaveFields_pass0, "VAL")
}

record(ai, "$(P){$(DEV)-Ax:$(AX)}$(CMD):RBV") {
  field(DESC, "Axis $(AX) $(CMD) readback")
  field(DTYP, "stream")
  field(INP, "@pump33dds.proto getVolAx($(P), $(DEV), $(AX), $(CMD)) $(PORT)")
  field(PREC, "5")
  field(SDIS, "$(P)$(R)DISABLE")
  field(SCAN,"1 second")
}

