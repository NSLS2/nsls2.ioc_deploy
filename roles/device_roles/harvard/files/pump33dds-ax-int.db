record(ao, "$(P){$(DEV)-Ax:$(AX)}$(CMD)") {
  field(DESC, "Axis $(AX) $(CMD)")
  field(DTYP, "stream")
  field(FLNK, "$(P){$(DEV)-Ax:$(AX)}$(CMD):RBV")
  field(OUT, "@pump33dds.proto setIntAx($(P), $(DEV), $(AX), $(CMD)) $(PORT)")
  field(PREC, "0")
  field(EGU, "$(EGU)")
  field(DRVH, "100")
  field(DRVL, "0")
  field(SDIS, "$(P)$(R)DISABLE")
  info(autosaveFields_pass0, "VAL")
}

record(ai, "$(P){$(DEV)-Ax:$(AX)}$(CMD):RBV") {
  field(DESC, "Axis $(AX) $(CMD) readback")
  field(DTYP, "stream")
  field(INP, "@pump33dds.proto getIntEGUAx($(P), $(DEV), $(AX), $(CMD), $(EGU)) $(PORT)")
  field(PREC, "0")
  field(EGU, "$(EGU)")
  field(SDIS, "$(P)$(R)DISABLE")
}


record(mbbi, "$(P){$(DEV)-Ax:$(AX)}$(CMD):RBV:EGU") {
  #field(DESC, "Pump Status")
  field(ZRVL, "0")
  field(ZRST, "$(EGU)")
  field(ONVL, "1")
  field(ONST, "Invalid")
  field(SDIS, "$(P)$(R)DISABLE")
}
